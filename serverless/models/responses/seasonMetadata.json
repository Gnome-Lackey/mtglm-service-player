{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "PlayerSeasonMetadataResponseSchema",
  "required": ["id", "seasonWins", "seasonLosses", "totalWins", "totalLosses"],
  "properties": {
    "id": {
      "type": "string",
      "title": "PlayerSeasonMetadataId"
    },
    "seasonWins": {
      "type": "integer",
      "title": "PlayerSeasonMetadataSeasonWins"
    },
    "seasonLosses": {
      "type": "integer",
      "title": "PlayerSeasonMetadataSeasonLosses"
    },
    "totalWins": {
      "type": "integer",
      "title": "PlayerSeasonMetadataTotalWins"
    },
    "totalLosses": {
      "type": "integer",
      "title": "PlayerSeasonMetadataTotalLosses"
    },
    "matches": {
      "type": "array",
      "title": "PlayerSeasonMetadataMatchIds",
      "items": {
        "type": "string"
      }
    },
    "playedOpponents": {
      "type": "array",
      "title": "PlayerSeasonMetadataPlayerIds",
      "items": {
        "type": "string"
      }
    }
  }
}
